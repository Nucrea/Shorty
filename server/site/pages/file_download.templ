package pages

templ FileDownload(url string) {
    <div class="flex flex-col bg-white rounded-md shadow-lg p-4">
        <b class="mb-2">Thank you for using Shorty!</b>
        <a href={ templ.URL(url) } target="_self" class="font-medium text-blue-600 underline dark:text-blue-500 hover:no-underline">To download the file, use this link</a>
    </div>
}